<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:vm="clr-namespace:SIDE.ViewModels"
        mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="120"
        x:DataType="vm:SetBuildStringViewModel"
        x:Class="SIDE.Views.SetBuildStringView"
        Title="SetBuildStringView">

  <StackPanel Margin="10" Spacing="10">
    <TextBlock Text="Set Terminal Command:" />
    <TextBox Text="{Binding TerminalString}"/>
    <Button HorizontalAlignment="Right" Width="80" Click="ButtonOk_Click">OK</Button>
  </StackPanel>

</Window>
